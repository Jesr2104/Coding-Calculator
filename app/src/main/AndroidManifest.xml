<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.just_jump.coding_calculator">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_app"
        android:supportsRtl="true"
        android:theme="@style/CodingCalculatorTheme">
        <activity android:name="com.just_jump.coding_calculator.ExtraCalculations" />
        <activity android:name="com.just_jump.coding_calculator.com.just_jump.coding_calculator.Converter"/>
        <activity android:name="com.just_jump.coding_calculator.NumericalSystems" />
        <activity android:name="com.just_jump.coding_calculator.GammaColors" />
        <activity android:name="com.just_jump.coding_calculator.PaletteColors" />
        <activity android:name="com.just_jump.coding_calculator.com.just_jump.coding_calculator.ColorCode" />
        <activity android:name="com.just_jump.coding_calculator.com.just_jump.coding_calculator.Calculator" />
        <activity android:name="com.just_jump.coding_calculator.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>